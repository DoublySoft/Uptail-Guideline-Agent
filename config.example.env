NODE_ENV="development"

# Database Configuration
DATABASE_URL="postgresql://uptail_user:uptail_password@localhost:5432/uptail_guidelines"

# Prisma Configuration
PRISMA_GENERATE_DATAPROXY=false

# LLM Provider Configuration
LLM_PROVIDER="openai"  # "openai" or "vercel"

# OpenAI Configuration
OPENAI_API_KEY="your_openai_api_key_here"
OPENAI_BASE_URL="https://api.openai.com/v1"
OPENAI_MODEL="gpt-4o-mini"
OPENAI_EMBEDDING_MODEL="text-embedding-3-small"

# Vercel AI Configuration
VERCEL_AI_API_KEY="your_vercel_ai_api_key_here"
VERCEL_AI_BASE_URL="https://api.vercel.ai/v1"
VERCEL_AI_MODEL="anthropic/claude-3.5-sonnet"
VERCEL_AI_EMBEDDING_MODEL="text-embedding-3-small"
